{
  "name": "tac-browser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:copy && ng serve --aot",
    "build": "npm run build:copy && npm run internalBuild",
    "internalBuild": "ng build --aot --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:copy": "copyfiles -u 3 node_modules/jquery/dist/jquery.min.js src/js",
    "publishImage": "docker build -f Dockerfile-prod -t registry.johannes-wirth.de/tac-browser:prod . && docker push registry.johannes-wirth.de/tac-browser:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.12",
    "@angular/common": "~8.2.12",
    "@angular/compiler": "~8.2.12",
    "@angular/core": "~8.2.12",
    "@angular/forms": "~8.2.12",
    "@angular/platform-browser": "~8.2.12",
    "@angular/platform-browser-dynamic": "~8.2.12",
    "@angular/router": "~8.2.12",
    "@clr/angular": "^2.3.0",
    "@clr/icons": "^2.3.0",
    "@clr/ui": "^2.3.0",
    "@webcomponents/custom-elements": "^1.3.0",
    "core-js": "^3.3.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "ngx-cookie": "~4.0.0",
    "rxjs": "~6.5.3",
    "rxjs-compat": "~6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.14",
    "@angular/cli": "8.3.14",
    "@angular/compiler-cli": "8.2.12",
    "@angular/language-service": "8.2.12",
    "@compodoc/compodoc": "^1.1.11",
    "@schematics/angular": "8.3.14",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.7",
    "codelyzer": "^5.2.0",
    "copyfiles": "^2.1.1",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
