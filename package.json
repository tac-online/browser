{
  "name": "tac-browser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:copy && ng serve --aot",
    "build": "npm run build:copy && npm run internalBuild",
    "internalBuild": "ng build --aot --base-href /tac/ --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:copy": "copyfiles -u 3 node_modules/jquery/dist/jquery.min.js src/js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/http": "7.2.14",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/router": "8.0.0",
    "@nebular/auth": "^3.5.0",
    "@nebular/theme": "^3.5.0",
    "core-js": "^2.6.9",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "nebular-icons": "^1.1.0",
    "ngx-bootstrap": "^3.3.0",
    "rxjs": "6.5.2",
    "rxjs-compat": "^6.5.2",
    "zone.js": "^0.9.1"
