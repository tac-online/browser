    "codelyzer": "^4.5.0",
    "copyfiles": "^2.1.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.4.2",
