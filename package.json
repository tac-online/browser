{
  "name": "tac-browser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:copy && ng serve --aot",
    "build": "npm run build:copy && npm run internalBuild",
    "internalBuild": "ng build --aot --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:copy": "copyfiles -u 3 node_modules/jquery/dist/jquery.min.js src/js",
    "publishImage": "docker build -f Dockerfile-prod -t registry.johannes-wirth.de/tac-browser:prod . && docker push registry.johannes-wirth.de/tac-browser:prod",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@clr/angular": "^3.0.1",
    "@clr/core": "^3.0.0",
    "@clr/icons": "^3.0.1",
    "@clr/ui": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "core-js": "^3.6.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "ngx-cookie": "~4.0.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@compodoc/compodoc": "^1.1.11",
    "@schematics/angular": "~9.1.0",
    "@types/node": "^12.12.31",
    "codelyzer": "^5.2.2",
    "copyfiles": "^2.2.0",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
